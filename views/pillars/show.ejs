<% include ../partials/school/header %>
<% include ../partials/school/side-header %>

      <h2 class="ui header"><i class="bar chart icon"></i>Pillars Grafieken</h2>
      <h2 class="ui right aligned header">
        <a href="/scholen/<%=school._id%>/pillars/instellingen">
        <i class="setting icon"></i>
        </a>
      </h2>
      <!--TOTAAL-->
      <h2 style="display:none;" id="totaalHardware"><%=result.totaal.hardware %></h2>
      <h2 style="display:none;" id="totaalSoftware"><%=result.totaal.software %></h2>
      <h2 style="display:none;" id="totaalDeskundigheid"><%=result.totaal.deskundigheid %></h2>
      <h2 style="display:none;" id="totaalOrganisatie"><%=result.totaal.organisatie %></h2>
      <div class="ui horizontal divider">
        Pillars Score
      </div>
      <canvas id="totaal"></canvas>
      <!--HARDWARE-->
      <h2 style="display:none;" id="scoreComputersPerLeerling"><%=result.hardware.computersPerStudent %></h2>
      <h2 style="display:none;" id="scoreDigibordenPerKlaslokaal"><%=result.hardware.digitalBordsPerClassroom %></h2>
      <h2 style="display:none;" id="scoreNetwerk"><%=result.hardware.network %></h2>
      <h2 style="display:none;" id="scorePortableComputersPerLeerling"><%=result.hardware.portableComputersPerSchool %></h2>
      <div class="ui horizontal divider">
        Hardware
      </div>
      <canvas id="hardware"></canvas>
      <!--DIGITALE LEERMIDDELEN-->
      <% Object.keys(result.software).forEach(function(vak){%>
          <h2 style="display:none;" id="<%=vak%>"><%=result.software[vak] %></h2>
      <%})%>
      <div class="ui horizontal divider">
        Digitale Leermiddelen
      </div>
      <canvas id="software"></canvas>
      <!--DESKUNDIGHEID-->
      <h2 style="display:none;" id="beoordeeldeDeskundigheid"><%=result.deskundigheid.beoordeeldeDeskundigheid %></h2>
      <h2 style="display:none;" id="gemiddeldeEffectiviteit"><%=result.deskundigheid.gemiddeldeEffectiviteit %></h2>
      <h2 style="display:none;" id="ondersteuningNodig"><%=result.deskundigheid.ondersteuningNodig %></h2>
      <h2 style="display:none;" id="ictGeletterheid"><%=result.deskundigheid.ictGeletterheid %></h2>
      <h2 style="display:none;" id="pedagogischDidactisch"><%=result.deskundigheid.pedagogischDidactisch %></h2>
      <h2 style="display:none;" id="werkenSchooltext"><%=result.deskundigheid.werkenSchooltext %></h2>
      <h2 style="display:none;" id="persoonlijkeOntwikkeling"><%=result.deskundigheid.persoonlijkeOntwikkeling %></h2>
      <h2 style="display:none;" id="instrumenteleVaardigheden"><%=result.deskundigheid.instrumenteleVaardigheden %></h2>
      <h2 style="display:none;" id="informatieVaardigheden"><%=result.deskundigheid.informatieVaardigheden %></h2>
      <h2 style="display:none;" id="mediaVaardigheid"><%=result.deskundigheid.mediaVaardigheid %></h2>
      <div class="ui horizontal divider">
        Deskundigheid
      </div>
      <canvas id="deskundigheid"></canvas>
      <!--ORGANISATIE-->
      <h2 style="display:none;" id="organisatorischeOvereenstemming"><%=result.organisatie.organisatorischeOvereenstemming %></h2>
      <h2 style="display:none;" id="netwerkbeheer"><%=result.organisatie.netwerkbeheer %></h2>
      <h2 style="display:none;" id="incidentmelder"><%=result.organisatie.incidentmelder %></h2>
      <h2 style="display:none;" id="onderwijskundigICTer"><%=result.organisatie.onderwijskundigICTer %></h2>
      <h2 style="display:none;" id="ictInkoper"><%=result.organisatie.ictInkoper %></h2>
      <div class="ui horizontal divider">
        Organisatie
      </div>
      <canvas id="organisatie"></canvas>

<% include ../partials/school/side-footer %>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.min.js"></script>
<script src="/../../js/app.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/components/form.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/components/dropdown.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/components/transition.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/components/tab.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.min.js"></script>
<script src="/../../js/pillars.js"></script>

</body>
</html>