    <a class="item" data-tab="eighth">8. B.T.</a>
    <a class="item" data-tab="nineth">9. G.B.</a>
    <a class="item" data-tab="tenth">10. B.I.V.</a>
  </div>
  <div class="ui bottom attached tab segment active" data-tab="first">
      <%- include('new/part1')%>
  </div>
  <div class="ui bottom attached tab segment" data-tab="second">
      <%- include('new/part2')%>
