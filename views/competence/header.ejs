<h2 class="ui centered header">
  <%= title %>
</h2>
<%if(typeof classroom==='undefined'){%>
	<div class="ui small basic icon buttons">
		<a class="ui <%if(url===("/schools/"+school._id+"/" + competenceStandardKey)){ %> active <%}%> icon button" 
	          href="/schools/<%=school._id%>/<%=competenceStandardKey%>"
	          data-content="Resultaten <%=title%>"><i class="info icon"></i></a>
		<a class="ui <%if(url===("/schools/"+school._id+"/" + competenceStandardKey + "/score-table")){ %> active <%}%> icon button" 
	          href="/schools/<%=school._id%>/<%=competenceStandardKey%>/score-table"
	          data-content="Tabel met score resultaten"><i class="table icon"></i></a>
		<a class="ui <%if(url===("/schools/"+school._id+"/" + competenceStandardKey + "/survey-analytics")){ %> active <%}%> icon button" 
		      href="/schools/<%=school._id%>/<%=competenceStandardKey%>/survey-analytics"
		      data-content="Grafieken van vragen en antwoorden"><i class="chart pie icon"></i></a>
		<a class="ui  <%if(url===("/schools/"+school._id+"/" + competenceStandardKey +"/boxplot-analysis")){ %> active <%}%>icon button"
			href="/schools/<%=school._id%>/<%=competenceStandardKey%>/boxplot-analysis"
		      data-content="Boxplot analyse"><i class="chart bar icon"></i></a>
		<%if(showManageClassroomsButton){%>
			<a class="ui button" href="/schools/<%=school._id%>/classroom" 
			data-content="Klassen beheren"><i class="student icon"></i></a>
		<%}%>
	</div>
<%} else {%>
	<div class="ui small basic button" onclick="goBack()"><i class="arrow left icon"></i></div>
<%}%>
<div class="ui divider"></div>
